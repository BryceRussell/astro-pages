	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup"
	},
	"peerDependencies": {
		"astro": ">=3"
	},
	"devDependencies": {
		"@types/node": "^22.10.1",
		"tsup": "^8.3.5"
